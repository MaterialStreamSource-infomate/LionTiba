        '***********************
        ObjDb.SQL = strSQL.ToString
        ObjDb.Parameter = objParameter
        objDataSet.Clear()
        strDataSetName = "TMST_ST"
        ObjDb.GetDataSet(strDataSetName, objDataSet)
